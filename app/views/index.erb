<div id="page-index">
  <div class="row">

    <div class="col-sm-8">
      <div id="navigation">
        <form>
          <button type="submit">Sign up</button>
          <button type="submit">Log in</button>
          <a href="/new" role="button">BECOME A HOST</a>
        </form>
      </div>
      <div id="search-box">
        <form method="get" action="/">
          <input type="text" name="city" placeholder="e.g. Vancouver">
          <button type="submit">Go!</button>
        </form>
      </div>
      <div id="instagram-box">
        <ul id="instagram-images" class="my-new-list">
        </ul>
      </div>
      <div id="footer">
        <small>Design by Aiste | 2015.</small>
      </div>
    </div>

    <div class="col-sm-4">
      <div id="map-canvas" class="map">
      </div>
    </div>

  </div>
</div>



















